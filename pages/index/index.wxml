<view class="data-v-57280228">
    <view class="content data-v-57280228">
        <view class="swiper-box data-v-57280228">
            <view class="data-v-57280228" style="box-shadow:0px 5px 15px #757575;">
                <u-swiper bind:__l="__l" bind:change="__e" borderRadius="0" class="data-v-57280228" data-event-opts="{{[ [ '^change',[ ['swiperChange'] ] ] ]}}" height="386" list="{{list}}" mode="none" name="pic_path" vueId="8dd740cc-1"></u-swiper>
            </view>
            <view class="sound-box data-v-57280228">
                <image class="sound data-v-57280228" src="../../static/sound.png"></image>
                <view class="u-line-1 u-m-l-15 sound-text data-v-57280228">{{welcomeInfo}}</view>
            </view>
            <view class="u-flex u-row-center u-m-t-15 data-v-57280228">
                <view class="{{['u-indicator-item-round','data-v-57280228',index==current?'u-indicator-item-round-active':'']}}" wx:for="{{list}}" wx:key="index"></view>
            </view>
        </view>
        <view class="nav-box data-v-57280228">
            <view class="title data-v-57280228">政务公开</view>
            <view class="grid-box data-v-57280228">
                <view class="data-v-57280228" style="width:50%;" wx:for="{{gridList}}" wx:key="index">
                    <view bindtap="__e" class="grid-item data-v-57280228" data-event-opts="{{[ [ 'tap',[ [ 'redirect',[index] ] ] ] ]}}">
                        <image class="gridicon data-v-57280228" src="{{item.image}}"></image>
                        <text class="text data-v-57280228">{{item.text}}</text>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <foot-copy-right bind:__l="__l" class="data-v-57280228" vueId="8dd740cc-2"></foot-copy-right>
</view>
